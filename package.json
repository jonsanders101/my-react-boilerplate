{
  "name": "My React Boilerplate",
  "version": "1.0.0",
  "description": "Simple React project boilerplate",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "prod": "NODE_ENV=production node tools/prodServer.js",
    "dev": "run-p serveDev lint:watch",
    "start": "npm run dev",
    "serveDev": "NODE_ENV=development node tools/devServer.js",
    "lint": "node_modules/.bin/esw src tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "jasmine"
